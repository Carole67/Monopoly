<Window x:Class="Monopoly.View.Component.navigationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Monopoly.View.Component"
        mc:Ignorable="d"
        Title="navigationWindow" Height="450" Width="800">


    <StackPanel HorizontalAlignment="Left" Height="66.4" VerticalAlignment="Top" Width="123.2">
        <Button x:Name="btnBack" Height="30" Width="30" HorizontalAlignment="Right" BorderBrush="White" Click="btnBack_Click" RenderTransformOrigin="-1.107,-0.667">
            <Button.Background>
                <ImageBrush ImageSource="../Pictures/back.png"/>
            </Button.Background>
        </Button>
        <Button x:Name="btnShutDown" HorizontalAlignment="Right" Height="30" Width="30" BorderBrush="White" Click="btnShutDown_Click" >
            <Button.Background>
                <ImageBrush ImageSource="../Pictures/shut_down.png"/>
            </Button.Background>
        </Button>
    </StackPanel>


</Window>
